{
  "name": "DnD Manager",
  "version": "1.0.0",
  "description": "An app to manage your DnD characters and search for information on monsters and spells",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "start": "npm run build && node server/server.js",
    "hrc": "cd client && npm run hotReload",
    "hrs": "start http://localhost:5000 & nodemon server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm run dev\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build",
    "render-build": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newprice247/DnDmanager.git"
  },
  "keywords": [
    "dungeons",
    "dragons",
    "dnd",
    "manager",
    "character",
    "spell",
    "monster",
    "react"
  ],
  "author": "newprice247",
  "bugs": {
    "url": "https://github.com/newprice247/DnDmanager/issues"
  },
  "homepage": "https://github.com/newprice247/DnDmanager#readme",
  "dependencies": {
    "axios": "^1.6.2",
    "faker": "^5.5.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  }
}
